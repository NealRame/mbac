{
    "name": "mbac",
    "version": "0.0.1",
    "authors": [
        "NealRame <contact@nealrame.com>"
    ],
    "license": "MIT",
    "private": true,
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components",
        "test",
        "tests"
    ],
    "dependencies": {
        "backbone": "~1.1.2",
        "jquery": "~2.1.1",
        "foundation": "5.4.2",
        "requirejs": "~2.1.14",
        "react": "~0.11.0",
        "modernizr": "~2.8.3",
        "font-awesome": "~4.2.0",
        "monosocialiconsfont": "*"
    },
    "exportsOverride": {
        "backbone": {
            "app/client/js/libs": "backbone.js"
        },
        "fastclick": {
            "app/client/js/libs": "fastclick.js"
        },
        "font-awesome": {
            "app/client/sass/libs/font-awesome": "scss/**/*.scss",
            "public/fonts": "fonts/*"
        },
        "foundation": {
            "app/client/sass/libs/foundation": "scss/*.scss",
            "app/client/sass/libs/foundation/foundation": "scss/foundation/*.scss",
            "app/client/sass/libs/foundation/foundation/components": "scss/foundation/components/*.scss",
            "app/client/js/libs": "js/foundation.js"
        },
        "jquery": {
            "app/client/js/libs": "dist/jquery.js"
        },
        "jquery-placeholder": {
            "app/client/js/libs": "jquery.placeholder.js"
        },
        "jquery.cookie": {
            "app/client/js/libs": "jquery.cookie.js"
        },
        "modernizr": {
            "app/client/js/libs": "modernizr.js"
        },
        "monosocialiconsfont": {
            "public/fonts": "MonoSocialIconsFont-*"
        },
        "react": {
            "app/client/js/libs": "react.js"
        },
        "requirejs": {
            "app/client/js/libs": "require.js"
        },
        "underscore": {
            "app/client/js/libs": "underscore.js"
        }
    }
}
